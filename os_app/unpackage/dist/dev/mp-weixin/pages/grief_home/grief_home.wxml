<view class="grief_home"><block wx:if="{{info.gone_user_details}}"><view class="grief_home_title"><view class="grief_home_title_text"></view><view class="grief_home_title_music"><image src="{{ifplay?'../../static/images/yy.png':'../../static/images/jyy.png'}}" data-event-opts="{{[['tap',[['playMusic',['$event']]]]]}}" bindtap="__e"></image></view><view data-event-opts="{{[['tap',[['openShare',['$event']]]]]}}" class="grief_home_title_share" bindtap="__e"><image src="../../static/images/fx.png"></image></view></view></block><view class="grief_home_content"><swiper class="grief_home_swiper_box" current="{{current_page}}"><block wx:for="{{html_list}}" wx:for-item="v" wx:for-index="i" wx:key="i"><swiper-item class="grief_home_swiper_item"><u-parse vue-id="{{'43abf7e0-1-'+i}}" content="{{v}}" bind:__l="__l"></u-parse></swiper-item></block></swiper></view><view class="num_box"><view class="icon1"><image src="../../static/images/jx1.png" mode="aspectFit"></image><view class="num_info"><view class="num_title">敬香</view><view class="num">{{''+info.actions.jx+'人'}}</view></view></view><view class="icon1"><image src="../../static/images/ma1.png" mode="aspectFit"></image><view class="num_info"><view class="num_title">默哀</view><view class="num">{{''+info.actions.ma+'人'}}</view></view></view></view><view class="grief_home_footer"><view data-event-opts="{{[['tap',[['jx',['$event']]]]]}}" class="footer_icon" bindtap="__e"><image src="../../static/images/jx.png"></image><view>祭拜</view></view><view data-event-opts="{{[['tap',[['attention',['$event']]]]]}}" class="footer_icon" bindtap="__e"><image src="../../static/images/jx.png"></image><view>关注</view></view></view><block wx:if="{{qrCode_show}}"><view data-event-opts="{{[['touchmove',[['mask_touchmove',['$event']]]]]}}" class="popup_qrcode_mask" catchtouchmove="__e"><view class="popup_qrcode"><image class="popup_qrcode_image" src="{{qrCode_url}}" mode="aspectFit" show-menu-by-longpress="{{true}}"></image><view class="popup_qrcode_text