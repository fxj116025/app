<view class="worship_header" style="{{('background-image:url('+bgImg+')')}}"><view class="top_btn"><view class="top_btn_yy"><image class="{{[is_play?'active_yy':'']}}" src="{{YY}}" data-event-opts="{{[['tap',[['is_music',['$event']]]]]}}" bindtap="__e"></image></view><view class="top_btn_num"><image src="../../static/images/fx2.png"></image><view class="fx_num">{{info.user_bless}}</view></view></view><view class="left_col"><block wx:for="{{left}}" wx:for-item="v" wx:for-index="i" wx:key="i"><view class="left_col_row" style="{{'background-color:'+(leftBtn)+';'}}"><image src="{{v.icon}}"></image><view class="text">{{v.title}}</view></view></block></view><image class="icon" style="{{'top:'+(imgTop+'rpx')+';'}}" src="{{info.gone_user_avatar}}" mode="aspectFill"></image><view class="info" style="{{'top:'+(infotop+'rpx')+';'}}"><view class="name">{{info.gone_user_name}}</view><view class="sex">{{info.gender===0?'女':'男'}}</view><view class="time">{{info.gone_user_years}}</view></view></view>